class Solution {
public:
    int maximumStrongPairXor(vector<int>& nums) {
        int n =nums.size();
        int maxi=0;
        for(int i=0;i<n;i++){
            for(int j=i;j<n;j++){
                int diff=abs(nums[i]-nums[j]);
                int mini=min(nums[i],nums[j]);
                if(diff<=mini){
                    int val= nums[i]^nums[j];
                    maxi=max(maxi,val);
                }
            }
        }
        return maxi;
    }
};
