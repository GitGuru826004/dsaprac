class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int>res;
        for(int i=0;i<nums.size();i++){
            int x=abs(nums[i]);
            if(nums[x-1]<0)res.push_back(x);
            else nums[x-1]*=-1;
        }
        return res;
    }
};

//Minimum possible value: 1
//Maximum possible value: n
// But in C++ arrays, indices go from 0 … n-1.
// That means we need a mapping:
// value 𝑥⟶index 𝑥−1
